<?xml version='1.0' encoding='utf-8'?>
<widget id="br.com.docz.app" version="1.3.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Docz</name>
    <description>Docz</description>
    <author email="carlosfeijao@gmail.com" href="http://www.ferramentadigital.com.br">
        Carlos Feij√£o
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="www/images/icons/32x32.png" />
        <icon density="mdpi" src="www/images/icons/64x64.png" />
        <icon density="hdpi" src="www/images/icons/64x64.png" />
        <icon density="xhdpi" src="www/images/icons/128x128.png" />
        <icon density="xxhdpi" src="www/images/icons/128x128.png" />
        <icon density="xxxhdpi" src="www/images/icons/128x128.png" />
    </platform>
    <preference name="AndroidPersistentFileLocation" value="Compatibility" />
    <plugin name="phonegap-plugin-barcodescanner" spec="^7.0.3">
        <variable name="ANDROID_SUPPORT_V4_VERSION" value="27.+" />
    </plugin>
    <plugin name="cordova-plugin-whitelist" spec="https://github.com/apache/cordova-plugin-whitelist.git" />
    <plugin name="cordova-plugin-dialogs" spec="^2.0.1" />
    <plugin name="cordova-plugin-tts" spec="https://github.com/vilic/cordova-plugin-tts.git" />
    <plugin name="cordova-plugin-media" spec="^5.0.2" />
    <plugin name="cordova-plugin-device" spec="^2.0.1" />
    <plugin name="cordova-sqlite-storage" spec="^2.3.3" />
    <plugin name="cordova-plugin-googleplus" spec="https://github.com/EddyVerbruggen/cordova-plugin-googleplus">
        <variable name="REVERSED_CLIENT_ID" value="1039865123784-6ekd1te0buu9ump2jjiqi5radd2384ag.apps.googleusercontent.com" />
        <variable name="WEB_APPLICATION_CLIENT_ID" value="1039865123784-6ekd1te0buu9ump2jjiqi5radd2384ag.apps.googleusercontent.com" />
        <variable name="PLAY_SERVICES_VERSION" value="11.8.0" />
    </plugin>
    <plugin name="cordova-plugin-camera" spec="^4.1.0">
        <variable name="ANDROID_SUPPORT_V4_VERSION" value="27.+" />
    </plugin>
    <engine name="android" spec="^7.0.0" />
    <engine name="browser" spec="^5.0.3" />
    <engine name="ios" spec="^4.5.5" />
</widget>
